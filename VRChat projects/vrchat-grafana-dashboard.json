    {
      "id": 20,
      "title": "Friends Detail \u2014 World & Avatar",
      "type": "table",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 26
      },
      "targets": [
        {
          "expr": "vrchat_friend_detail",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true,
              "Value": true
            },
            "renameByName": {
              "display_name": "Friend",
              "status": "Status",
              "platform": "Platform",
              "world_name": "World",
              "avatar_name": "Avatar"
            },
            "indexByName": {
              "display_name": 0,
              "status": 1,
              "world_name": 2,
              "avatar_name": 3,
              "platform": 4
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "filterable": true
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Status"
            },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "join me": {
                        "text": "\ud83d\udfe2 Join Me",
                        "color": "green"
                      }
                    }
                  },
                  {
                    "type": "value",
                    "options": {
                      "active": {
                        "text": "\ud83d\udfe2 Active",
                        "color": "green"
                      }
                    }
                  },
                  {
                    "type": "value",
                    "options": {
                      "ask me": {
                        "text": "\ud83d\udfe1 Ask Me",
                        "color": "yellow"
                      }
                    }
                  },
                  {
                    "type": "value",
                    "options": {
                      "busy": {
                        "text": "\ud83d\udd34 Busy",
                        "color": "red"
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "World",
            "desc": false
          }
        ]
      }
